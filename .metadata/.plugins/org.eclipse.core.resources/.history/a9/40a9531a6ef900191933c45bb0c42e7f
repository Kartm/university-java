package Class03;

public class Arrays {
	public static void main(String[] args) {
		int[][] arr = {{1, 4, 17}, {7, 12}, {3, 15, 19, 22}};
		System.out.println(isAscending(arr[0]));
		System.out.println(isAscending(arr[1]));
		System.out.println(isAscending(arr[2]));
	}
	
	// N?
	
	public static boolean isAscending(int[] arr) {
		for(int i = 0; i < arr.length - 1; i++) {
			if(arr[i] > arr[i+1]) {
				return false;
			}
		}
		return true;
	}
}
