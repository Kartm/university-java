package arithmeticOperations;

import java.util.Scanner;

public class Task4 {
	public static void main(String[] args) {
		// TODO auto-generated method stub
		int sum = 0;
		int elemNo = 0;
		Scanner sc = new Scanner(System.in);
		while(sc.hasNext() && sc.next().equals("^Z")) {
			String currElem = sc.next();
			try {
				int currElemInteger = Integer.parseInt(currElem);
				sum = sum + currElemInteger;
				elemNo=elemNo+ 1;
			}
			catch (Exception e) {
				System.out.println("This is not a number: " + currElem);
			}
			
		}
		if(elemNo == 0) {
			System.err.print("No data available\n");
			System.exit(1);
		}
		sc.close();
		System.out.print("the sum of all integers");
		System.out.print(" is " + sum + "\n");
		System.exit(0);
	}
}
